{
    "name": "presendoo-embed",
    "version": "1.0.0",
    "description": "Presendoo embed script for npm (typed) and CDN (plain JS).",
    "author": "Joren Lauryssen",
    "license": "MIT",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "unpkg": "dist/presendoo-embed.min.js",
    "jsdelivr": "dist/presendoo-embed.min.js",
    "scripts": {
        "build": "rollup -c",
        "prepublishOnly": "npm run build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,json,css,md}'"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "rollup": "^4.0.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "typescript": "^5.6.0",
        "typescript-eslint": "^8.42.0"
    }
}
